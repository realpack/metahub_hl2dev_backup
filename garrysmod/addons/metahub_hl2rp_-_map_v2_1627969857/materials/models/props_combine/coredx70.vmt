"UnlitTwoTexture"
{

	"$basetexture" "models/props_combine/coredx70_sheet"
	"$texture2" "models/props_combine/coredx70_sheet"
	"$model" "1"
	"$color" "[1 1 1]"

	"Proxies"
	{

		"Sine"
		{
			"resultVar" "$color[0]"
			"sineperiod" .1
			"sinemin" "0"
			"sinemax" "1"
		}

		"Sine"
		{
			"resultVar" "$color[1]"
			"sineperiod" 1
			"sinemin" ".5"
			"sinemax" "1"
		}

		"Sine"
		{
			"resultVar" "$color[2]"
			"sineperiod" 3
			"sinemin" ".9"
			"sinemax" "1"
		}


		"TextureScroll"
		{
			"texturescrollvar" "$texture2transform"
			"texturescrollrate" ".6"
			"texturescrollangle" "90"
		}
		"TextureScroll"
		{
			"texturescrollvar" "$basetexturetransform"
			"texturescrollrate" "-.5"
			"texturescrollangle" "93"
		}

	}
}


